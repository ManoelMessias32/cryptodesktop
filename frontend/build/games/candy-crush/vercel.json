{
  "headers": [
    {
      "source": "utils/(.*)\\.(png|jpg|jpeg|gif|webp)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/png"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}